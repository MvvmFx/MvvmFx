<?xml version="1.0"?>
<configuration>
	<system.diagnostics>
		<trace autoflush="true"/>
		<sources>
			<source name="MvvmFx.Windows" switchName="tracing">
				<listeners>
					<add name="textlistener"/>
				</listeners>
			</source>
		</sources>
		<sharedListeners>
			<add name="textlistener" initializeData="log.txt" type="System.Diagnostics.TextWriterTraceListener"/>
		</sharedListeners>
		<switches>
			<add name="tracing" value="Verbose"/>
		</switches>
	</system.diagnostics>
</configuration>
