  <connectionStrings>
    <add name="CslaSample" connectionString="Data Source=(LocalDB)\v11.0;AttachDbFilename=|DataDirectory|CslaSample.mdf;Integrated Security=True;Connection Timeout=300;" providerName="System.Data.SqlClient" />
