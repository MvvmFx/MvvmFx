      <frameworkAssembly assemblyName="Gizmox.WebGUI.Common" targetFramework="net45" />
      <frameworkAssembly assemblyName="Gizmox.WebGUI.Forms" targetFramework="net45" />
    </frameworkAssemblies>
  </metadata>
